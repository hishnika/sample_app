<% if @feed_items.any? %>
  <ol class="microposts">
    <%# here rails can't infer the collection based on the controller name %>
    <%# also when using collection, you have to specify it's a 'partial' %>
    <%= render partial: 'shared/feed_item', collection: @feed_items %>  
  </ol>
  <%= will_paginate @feed_items %>
<% end %>